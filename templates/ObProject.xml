<templateSet group="ObProject">
  <template name="banner" value="{% include 'theme/_element/_banner$MODIFICATOR$.html.twig' with {'bannerName': '$BANNER_NAME$'} %}" description="Встраивание баннера в код старницы" toReformat="false" toShortenFQNames="true">
    <variable name="MODIFICATOR" expression="enum(&quot;&quot;,&quot;__aside_top&quot;,&quot;__bottom&quot;,&quot;__header__desktop&quot;,&quot;__header__mobile&quot;,&quot;__post_list&quot;)" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <variable name="BANNER_NAME" expression="enum(&quot;Moscow&quot;,&quot;Kursk&quot;,&quot;Vladivostok&quot;,&quot;Sevastopol&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="group" value="@group $GROUP_NAME$" description="назначение тесту группы" toReformat="false" toShortenFQNames="true">
    <variable name="GROUP_NAME" expression="enum(&quot;registration&quot;, &quot;authorisation&quot;, &quot;subscription&quot;, &quot;email_campaign&quot;, &quot;service_email&quot;, &quot;tech_pages&quot;, &quot;remind_pwd&quot;, &quot;topic_approval&quot;, &quot;lang&quot;, &quot;url_generator&quot;, &quot;oborot_api&quot;, &quot;third_party_api&quot;, &quot;twig_ext&quot;, &quot;content_form&quot;, &quot;seo&quot;, &quot;advertisement&quot;, &quot;&quot;)" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP Comment" value="true" />
    </context>
  </template>
</templateSet>
